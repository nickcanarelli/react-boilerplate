{
  "name": "blank-web-application",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "2.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "web-vitals": "2.1.2"
  },
  "scripts": {
    "start": "cross-env PORT=6969 TARGET_ENV=local craco start",
    "start:storybook": "start-storybook -p 3001",
    "test": "cross-env TARGET_ENV=local craco test --watchAll=false",
    "test:watch": "cross-env TARGET_ENV=local craco test --watch",
    "test:ci": "cross-env TARGET_ENV=local craco test --watchAll=false --runInBand",
    "build": "cross-env TARGET_ENV=local craco build",
    "build:storybook": "build-storybook -o build-storybook",
    "lint": "eslint src .storybook *.js --fix",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "7.0.0",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/builder-webpack5": "6.5.16",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/node-logger": "6.5.16",
    "@storybook/preset-create-react-app": "4.1.2",
    "@storybook/react": "6.5.16",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.0.3",
    "@types/node": "17.0.6",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "autoprefixer": "10.4.0",
    "circular-dependency-plugin": "5.2.2",
    "cross-env": "7.0.3",
    "cssnano": "5.0.14",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "postcss": "8.4.5",
    "prettier": "2.5.1",
    "react-scripts": "5.0.0",
    "tailwindcss": "3.0.7",
    "typescript": "4.5.4",
    "webpack": "5"
  }
}
